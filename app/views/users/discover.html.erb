<h1><%=@user.name%>'s Discover Page</h1>

<%= button_to "Discover top rated movies", user_movies_path(@user), method: :get %>

<%= form_with url: user_movies_path(@user), method: :get do |form| %>
  <%= form.text_field :search_keyword, placeholder: "Enter keyword(s) to search by movie title" %>
  <%= form.submit "Search by movie title" %>
<% end %>